<%= form_for nonprofits_path do |f| %>
    <%= f.text_field :organization, placeholder: "Organization", required: true %><br/>
    <%= f.text_field :name, placeholder: "Contact name", required: true %><br/>
    <%= f.email_field :email, placeholder: "Email", required: true %><br/>
    <%= f.phone_field :phone, placeholder: "Phone", required: true %><br/>
    <%= f.text_field :username, placeholder: "Username", required: true %><br/>
    <%= f.password_field :password, placeholder: "Password", required: true, id:'see-password' %><span id="eye">&#128065;</span><br/>
    <%= f.submit 'Sign Up', class: "btn-standart" %>
<% end %>




