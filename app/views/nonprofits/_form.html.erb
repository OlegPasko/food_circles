<%= form_for nonprofits_path do |f| %>
    <%= f.text_field :organization, placeholder: "Organization" %><br/>
    <%= f.text_field :name, placeholder: "Name" %><br/>
    <%= f.email_field :email, placeholder: "Email" %><br/>
    <%= f.text_field :phone, placeholder: "Phone" %><br/>
    <%= f.text_field :username, placeholder: "Username" %><br/>
    <%= f.password_field :password, placeholder: "Password" %><br/>
    <%= submit_tag 'Sign Up', class: "btn-standart" %>
<% end %>