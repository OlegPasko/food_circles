<%= form_tag nonprofits_path, id: 'nonprofit-form' do |f| %>
    <%= f.text_field :organization, placeholder: "Organization" %><br/>
    <%= f.text_field :name, nil, placeholder: "Name" %><br/>
    <%= f.email_field :email, nil, placeholder: "Email" %><br/>
    <%= f.text_field :phone, nil, placeholder: "Phone" %><br/>
    <%= f.text_field :username, nil, placeholder: "Username" %><br/>
    <%= f.password_field :password, nil, placeholder: "Password" %><br/>
    <%= submit_tag 'Sign Up', class: "btn-standart" %>
<% end %>