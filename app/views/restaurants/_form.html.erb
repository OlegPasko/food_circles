<%= form_for restaurants_path do |f| %>
    <%= f.text_field :organization, placeholder: "Restaurant name" %><br/>
    <%= f.text_field :name, placeholder: "Contact name" %><br/>
    <%= f.email_field :email, placeholder: "Email" %><br/>
    <%= f.phone_field :phone, placeholder: "Phone" %><br/>
    <%= f.text_field :username, placeholder: "Username" %><br/>
    <%= f.password_field :password, placeholder: "Password", id:'see-password' %><span id="eye">&#128065;</span><br/>
    <%= f.submit 'Sign Up', class: "btn-standart" %>
<% end %>
